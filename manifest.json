{
  "manifest_version": 2,

  "name": "Sha1",
  "description": "gives a sha1 hash of something",
  "version": "1.0",
  "icons": {
        "128": "icon.png",
        "16": "icon.png",
        "48": "icon.png"
    },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
